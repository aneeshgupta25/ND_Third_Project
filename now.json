{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/weatherInfo",
      "dest": "server/index.js"
    },
    {
      "src": "/postWeather",
      "dest": "server/index.js"
    },
    {
      "src": "/all",
      "dest": "server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/website/"
    }
  ]
}



